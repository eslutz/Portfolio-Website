{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg,mp4,json}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["POST"],
      "rewrite": "/api/getCosmosData",
      "headers": {
        "Cache-Control": "public, max-age=86400, stale-if-error=86400"
      }
    },
    {
      "route": "/*.html",
      "headers": {
        "Cache-Control": "public, max-age=3600, stale-if-error=86400"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    },
    {
      "route": "/*.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    },
    {
      "route": "/*.jpg",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    },
    {
      "route": "/*.gif",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    },
    {
      "route": "/*.mp4",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable, stale-if-error=86400"
      }
    }
  ]
}
